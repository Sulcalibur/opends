{
  "extends": "./.nuxt/tsconfig.json",
  "include": ["src/**/*", "**/*.vue"],
  "exclude": ["node_modules", "dist", ".nuxt"],
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "strict": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"]
  }
}