{
  "name": "@opends/backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "migration:generate": "typeorm migration:generate -d src/infrastructure/database/data-source.ts",
    "migration:run": "typeorm migration:run -d src/infrastructure/database/data-source.ts",
    "migration:revert": "typeorm migration:revert -d src/infrastructure/database/data-source.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.952.0",
    "@aws-sdk/s3-request-presigner": "^3.952.0",
    "@fastify/cors": "^9.0.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/rate-limit": "^9.0.0",
    "@fastify/swagger": "^8.0.0",
    "@fastify/swagger-ui": "^2.0.0",
    "better-sqlite3": "^12.5.0",
    "bullmq": "^5.0.0",
    "dotenv": "^16.3.0",
    "fastify": "^4.0.0",
    "ioredis": "^5.3.0",
    "pg": "^8.11.0",
    "pino-pretty": "^13.1.3",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/pg": "^8.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "tsx": "^4.0.0",
    "typescript": "~5.3.0",
    "vitest": "^1.0.0"
  }
}
